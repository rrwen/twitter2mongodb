	"istanbul": "istanbul cover node_modules/tape/bin/tape tests/test.js",
	"test": "node ./tests/test.js"
  },
  "directories": {
	"test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrwen/twitter2mongodb.git"
  },
