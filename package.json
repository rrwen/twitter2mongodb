  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rrwen/twitter2mongodb/issues"
  },
  "homepage": "https://github.com/rrwen/twitter2mongodb#readme",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "documentation": "^5.3.3",
    "istanbul": "^0.4.5",
    "moment": "^2.19.1",
    "tape": "^4.8.0"
  }
}
